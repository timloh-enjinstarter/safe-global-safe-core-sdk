{
  "name": "@timloh-enjinstarter/safe-global-safe-web3-lib",
  "version": "0.1.0",
  "description": "Web3 library adapter to be used by Safe Core SDK",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "keywords": [
    "Ethereum",
    "Gnosis",
    "Safe",
    "SDK",
    "Web3"
  ],
  "scripts": {
    "typechain": "ts-node scripts/generateTypechainFiles.ts",
    "unbuild": "rimraf dist artifacts cache .nyc_output typechain",
    "build": "hardhat compile && yarn typechain && tsc",
    "format": "prettier --write \"{src,tests,hardhat,scripts}/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timloh-enjinstarter/safe-global-safe-core-sdk.git"
  },
  "author": "Gnosis Safe (https://gnosis-safe.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timloh-enjinstarter/safe-global-safe-core-sdk/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/timloh-enjinstarter/safe-global-safe-core-sdk#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@timloh-enjinstarter/safe-global-safe-contracts-v1.3.0": "npm:@timloh-enjinstarter/safe-global-safe-contracts@0.1.0",
    "@typechain/web3-v1": "^6.0.1",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.0",
    "@typescript-eslint/parser": "^5.36.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "hardhat": "^2.10.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@timloh-enjinstarter/safe-global-safe-core-sdk-types": "^0.1.0",
    "@timloh-enjinstarter/safe-global-safe-core-sdk-utils": "^0.1.0"
  },
  "peerDependencies": {
    "web3": "^1.7.0"
  }
}
